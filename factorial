#include <stdlib.h>
#include <stdio.h>

int main(void)
{
	int a;
	int b = 1;


	scanf("%d", &a);

	if ( a < 0 )
		printf("Number is wrong: %d", a);
	else if ( a == 0 )
	{
		printf("Factorial %d : %d", a, b);
	}
	else
	{
		for ( int i = 1; i <= a; i++ )
		b *= i;

		printf("Factorial %d : %d", a, b);
	}



	return 0;
}
