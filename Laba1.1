package com.company;

 import java.util.Scanner;

public class Job {
    public static void main(String[] args) {
        int Sum= 0;
        Scanner s = new Scanner(System.in);
        int[]arr= new int[5];
        int i=0;
        System.out.println("Введите элементы массива");
        while ( i < 5) {
            arr[i]=s.nextInt();
            Sum=Sum+arr[i];
            i++;
        }
        System.out.println("Сумма = "+Sum);
    }
}
